<Shell
    x:Class="KomodoPass.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:KomodoPass"
    Title="KomodoPass"
    Shell.FlyoutBehavior="Locked">

    <ShellContent Title="Home" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />

    <ShellContent Title="Config" ContentTemplate="{DataTemplate local:ConfigPage}" Route="ConfigPage" />
    
</Shell>